{
	"name": "myapp",
	"version": "0.0.0",
	"license": "MIT",
	"bin": "dist/cli.js",
	"type": "module",
	"engines": {
		"node": ">=20"
	},
	"scripts": {
		"build": "babel --out-dir=dist source",
		"dev": "babel --out-dir=dist --watch source",
		"test": "prettier --check . && xo && ava",
		"keymap": "node dist/utils/keymap-cli.js",
		"postinstall": "node dist/utils/keymap-cli.js"
	},
	"files": [
		"dist"
	],
	"dependencies": {
		"autobase": "^7.1.0",
		"b4a": "^1.6.7",
		"blind-pairing": "^2.3.1",
		"clipboardy": "^4.0.0",
		"corestore": "^7.0.23",
		"filesize": "^10.1.6",
		"hypercore": "^11.0.48",
		"hyperdb": "^4.9.4",
		"hyperdispatch": "^1.0.3",
		"hyperdrive": "^12.1.0",
		"hyperschema": "^1.10.4",
		"hyperswarm": "^4.10.1",
		"ink": "^4.1.0",
		"ink-big-text": "^2.0.0",
		"ink-gradient": "^3.0.0",
		"ink-text-input": "^6.0.0",
		"meow": "^11.0.0",
		"mime-types": "^2.1.35",
		"open": "^10.1.0",
		"react": "^18.2.0",
		"ready-resource": "^1.1.2",
		"string-width": "^7.2.0",
		"z32": "^1.1.0"
	},
	"devDependencies": {
		"@babel/cli": "^7.21.0",
		"@babel/preset-react": "^7.18.6",
		"@vdemedes/prettier-config": "^2.0.1",
		"ava": "^5.2.0",
		"chalk": "^5.2.0",
		"eslint-config-xo-react": "^0.27.0",
		"eslint-plugin-react": "^7.32.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"import-jsx": "^5.0.0",
		"ink-testing-library": "^3.0.0",
		"prettier": "^2.8.7",
		"xo": "^0.53.1"
	},
	"ava": {
		"environmentVariables": {
			"NODE_NO_WARNINGS": "1"
		},
		"nodeArguments": [
			"--loader=import-jsx"
		]
	},
	"xo": {
		"extends": "xo-react",
		"prettier": true,
		"rules": {
			"react/prop-types": "off"
		}
	},
	"prettier": "@vdemedes/prettier-config",
	"babel": {
		"presets": [
			"@babel/preset-react"
		]
	}
}
